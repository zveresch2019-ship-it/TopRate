    "react-native": "0.81.4",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2",
    "babel-preset-expo": "~54.0.0"
  },
  "private": true
